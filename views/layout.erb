<html lang="en">
  <head>
    <link href="/vendor/css/cloudgov-style.css" rel="stylesheet" />
  </head>

  <body>

<!-- start header -->
    <div class="usa-disclaimer">
      <div class="usa-grid">
        <span class="usa-disclaimer-official">An official website of the United States Government
          <img alt="US flag signifying that this is a United States Federal Government website" src="/vendor/img/us_flag_small.png">
        </span>
        <span class="usa-disclaimer-stage">This site is currently in alpha. <a href="https://18f.gsa.gov/dashboard/stages/#alpha">Learn more.</a></span>
      </div>
    </div>
    <header class="header">
      <div class="header-wrap">

        <!-- start logo -->
        <h2 class="header-title">
          <!-- <a href="https://cloud.gov" class="logo" title="Home">
            <svg class="logo">
              <use xlink:href="/vendor/img/cloudgov-sprite.svg#logo"/>
            </svg>
          </a> -->
          <a href="https://compliance-viewer.18f.gov" title="Home">Compliance Viewer</a>
        </h2>
        <!--end logo -->

        <!-- start top navigation -->
        <nav class="header-side">
          <ul class="nav">
            <li class="nav-link">
              <a href="/new">New Project?</a> 
            </li>
            <li class="nav-link usa-current">
              <a href="https://pages.18f.gov/before-you-ship/">Documentation</a> 
            </li>
            <li class="nav-link">
              <div class="button_wrapper">
                <button>Logout</button>
              </div>
            </li>
          </ul>
        </nav>
        <!-- end top navigation -->
      </div>

    </header>
    <!-- end header -->
    <main class="sidenav-parent">
      <!-- <aside id="sidebar" class="sidenav">abc</aside>-->
      <div class="sidenav-main" id="main-content">
        <div class="usa-content content">
        <%= yield %>
        </div>
      </div>
    </main>
    <footer class="usa-footer usa-footer-slim usa-sans" role="contentinfo">
      <div class="usa-footer-secondary_section">
        <div class="usa-grid">
          <div class="usa-footer-logo">
            <img class="usa-footer-slim-logo-img" src="/img/18f-logo.svg" alt="Logo image">
            <h3 class="usa-footer-slim-logo-heading">Compliance Viewer</h3>
            <h3 class="usa-footer-slim-logo-heading">View our code on Github</h3>
          </div>
        </div>
      </div>
    </footer>
    <script src="/vendor/js/cloudgov-style.js"></script>
  </body>
</html>
